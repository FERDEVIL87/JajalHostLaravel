{
    "version": 2,
    "framework": null,
    "builds": [
        {
            "src": "public/index.php",
            "use": "vercel-php@0.6.0"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(css|js|images|fonts|storage|favicon.ico)(.*)",
            "dest": "/public/$1$2"
        },
        {
            "src": "/(.*)",
            "dest": "/public/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://NAMA_PROYEK_ANDA.vercel.app",
        "APP_KEY": "@laravel-app-key",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp/views",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",
        "SESSION_LIFETIME": "120",

        "DB_CONNECTION": "mysql",
        "DB_HOST": "@db-host",
        "DB_PORT": "@db-port",
        "DB_DATABASE": "@db-database",
        "DB_USERNAME": "@db-username",
        "DB_PASSWORD": "@db-password",

        "FRONTEND_URL": "https://URL_FRONTEND_VUE_ANDA.vercel.app",
        "CORS_ALLOWED_ORIGINS": "https://URL_FRONTEND_VUE_ANDA.vercel.app"
    }
}